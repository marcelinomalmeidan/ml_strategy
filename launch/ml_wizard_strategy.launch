<launch>

  <node pkg="ml_strategy"
    type="ml_strategy"
    name="ml_strategy"
    output="screen">

    <!-- List of team quad names (first one is goalkeeper) -->
    <rosparam param="MyTeam"> ["harry",
                               "hermione",
                               "ron"] </rosparam>

    <rosparam param="InitialPosition"> [-6.0,  1.5, 2.0,
                                        -6.0,  0.0, 2.0,
                                        -6.0, -1.5, 2.0] </rosparam>

  </node>

</launch>